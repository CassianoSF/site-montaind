{
  "scripts": {
    "dev": "npx snowpack dev",
    "start": "npx snowpack dev",
    "build": "npx snowpack build"
  },
  "snowpack": {
    "mount": {
      "src": "/dist",
      "static": "/"
    },
    "devOptions": {
      "out": "public",
      "open": "default",
      "bundle": true
    },
    "install": [
      "imba"
    ],
    "installOptions": {},
    "buildOptions": {},
    "plugins": [
      [
        "imba-snowpack",
        {
          "entrypoints": [
            "app-root"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "imba-snowpack": "^0.0.6",
    "snowpack": "^2.7.6"
  },
  "dependencies": {
    "imba": "^2.0.0-alpha.96",
    "material-design-icons": "^3.0.1"
  }
}
